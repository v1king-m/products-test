{% extends 'v1mHelloBundle:Hello:layout.html.twig' %}
{% block title %}Store{% endblock %}
{% block content %}
<div class="content">
		{# <h2>Редактирование продукта</h2> #}
		<form action="{{ path('_login_check') }}" method="post">
			<table class="form_table"><tr>
			<td><label for="username">Имя:</label></td>
			<td><input type="text" id="username" name="_username" value="{{ last_username }}" /></td>
			</tr><tr>
			<td><label for="password">Пароль:</label></td>
			<td><input type="password" id="password" name="_password" /></td>
			</tr><tr>
			<td></td><td><input type="submit" name="login" value="Войти" /></td>
			</tr></table>
		</form>
</div>
{% block msg %}{% endblock %}
{% endblock %}

